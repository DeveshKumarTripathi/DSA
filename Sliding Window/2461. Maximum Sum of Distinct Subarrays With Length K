class Solution {
    public long maximumSubarraySum(int[] nums, int k) {
        HashSet<Integer> set = new HashSet<>();
        long MS = 0;
        long CS = 0;

        int i = 0;

        for(int j = 0; j < nums.length; j++){
            while(set.contains(nums[j])){
                CS -= nums[i];
                set.remove(nums[i]);
                i++;
            }
            CS += nums[j];
            set.add(nums[j]);

            if(set.size() == k){
                MS = Math.max(MS,CS);
                CS -= nums[i];
                set.remove(nums[i]);
                i++;
            }
        }
        return MS;
    }
}
