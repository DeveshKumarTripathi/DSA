class Solution {
    public boolean isSubsequence(String s, String t) {
        if(s.length() == 0)return true;
        int j = 0;
        int n = s.length();
        
        for(int i = 0; i < t.length(); i++){
            char ch = t.charAt(i);
            if(ch == s.charAt(j)){
                if(j != n-1)j++;
                else return true;
            }
        }return false;
    }
}
